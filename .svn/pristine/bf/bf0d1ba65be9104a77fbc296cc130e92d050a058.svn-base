<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    
    <style name="Transparent_Dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="ListView_style">
        <item name="android:fadeScrollbars">true</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:fadeEnabled">false</item>
        <item name="android:fadingEdgeLength">@dimen/fix_0</item>
        <item name="android:background">@null</item>
        <item name="android:listSelector">@android:color/transparent</item>
        <item name="android:scrollbars">none</item>
    </style>
    <style name="ImageStyle">
        <item name="android:adjustViewBounds">true</item>
        <item name="android:contentDescription">@string/contentDescript</item>
    </style>
    <style name="ImageStyle.CenterCrop">
        <item name="android:scaleType">centerCrop</item>
    </style>
    <style name="ImageStyle.Center">
        <item name="android:scaleType">center</item>
    </style>
    <style name="ImageStyle.CenterInside">
        <item name="android:scaleType">centerInside</item>
    </style>
    <style name="GoclibTitleBar">
        <item name="android:background">@drawable/goclib_selector_f55000000_n00000000</item>
    </style>
    <style name="GoclibTitleBar.Button">
        <item name="android:textSize">@dimen/h5</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:paddingLeft">@dimen/goclib_gap_medium_wide</item>
        <item name="android:paddingRight">@dimen/goclib_gap_medium_wide</item>
    </style>
    <style name="GoclibTitleBar.Button.Left"></style>
    <style name="GoclibTitleBar.Button.Right"></style>
    <style name="GoclibTitleBar.Button.Title"></style>
    <style name="GoclibTitleBar.ImageButton">
        <item name="android:scaleType">centerInside</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:paddingLeft">@dimen/goclib_gap_medium_wide</item>
        <item name="android:paddingRight">@dimen/goclib_gap_medium_wide</item>
    </style>
    <style name="GoclibTitleBar.ImageButton.Left"></style>
    <style name="GoclibTitleBar.ImageButton.Right"></style>
    <style name="GoclibTitleBar.ImageButton.Title"></style>
    
    <style name="GoclibWebView"></style>
</resources>