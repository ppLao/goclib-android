<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleBar">
        <attr name="goclib_titlebar_left" format="reference|string"></attr>
        <attr name="goclib_titlebar_right" format="reference|string"></attr>
        <attr name="goclib_titlebar_title" format="reference|string"></attr>
        <attr name="goclib_titlebar_title_enabled" format="boolean"></attr>
        <attr name="goclib_titlebar_title_text_size" format="dimension"></attr>
        <attr name="goclib_titlebar_title_text_color" format="color|string"></attr>
        <attr name="goclib_titlebar_right_enabled" format="boolean"></attr>
        <attr name="goclib_titlebar_right_text_size" format="dimension"></attr>
        <attr name="goclib_titlebar_right_text_color" format="color|string"></attr>
        <attr name="goclib_titlebar_left_enabled" format="boolean"></attr>
        <attr name="goclib_titlebar_left_text_size" format="dimension"></attr>
        <attr name="goclib_titlebar_left_text_color" format="color|string"></attr>
        <attr name="goclib_titlebar_default_gone" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="content" format="reference|string" />
    </declare-styleable>
    
    <declare-styleable name="Goclib_TitleBarButton">
        <attr name="content"></attr>
        <attr name="goclib_titlebar_button_type">
            <enum name="allVisible" value="0" />
            <enum name="allGone" value="1" />
            <enum name="imageview" value="2" />
            <enum name="textView" value="3" />
        </attr>
    </declare-styleable>

</resources>